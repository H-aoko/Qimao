.container {
  display: flex;
  flex-direction: column;
}
.topbar {
  position:absolute;
  margin-left: 0rpx;
  margin-top:0rpx;
  display: flex;
  flex-direction: row;
  height:110rpx;
  width:100%;
  background-color: white;
}
.menu {
  height:150rpx;
  position:absolute;
  margin-left: 40rpx;
}
.menuimage {
  height:90rpx;
  width: 108rpx;
}
.skip {
  height:150rpx;
  position: relative;
  left:200rpx;
}
.skipimage {
  height:100rpx;
  width:250rpx;
}
.menucontent {
  position: relative;
  right: 220rpx;
  top:110rpx;
  height:200rpx;
  width: 200rpx;
}
.blank {
  height:190rpx;
  width: 190rpx;
}
.home {
  height:80rpx;
  position:relative;
  right:70rpx;
  bottom: 200rpx;
}
.again {
  height:80rpx;
  position:absolute;
  left:50rpx;
  top: 0rpx;
}
.hint {
  height:80rpx;
  position:absolute;
  left:-30rpx;
  top: 70rpx;
}
.mute {
  height:80rpx;
  position:absolute;
  left:50rpx;
  top: 70rpx;
}
.btnimage {
  height:60rpx;
  width: 60rpx;
}
.swiperview {
  position:absolute;
  margin-left: 0rpx;
  margin-top:110rpx;
  width:100%;
}
.swiper {
  height: 1100rpx;
  width: 100%;
  /*position: absolute;
  margin-left: 0rpx;
