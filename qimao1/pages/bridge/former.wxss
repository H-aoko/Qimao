}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 9%;
  left: 2%;
  opacity: 0.85;
  z-index:99;
} 
.modal-dialog {
  overflow: visible;
  top: 11%;
  left: 0%;
  margin: 0rpx 5rpx;
  border-radius: 10%;
  position: fixed;
  z-index:99;
}

