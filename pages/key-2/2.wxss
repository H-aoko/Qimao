/* pages/2/2.wxss */
page{
  overflow: hidden;
  background: rgb(255, 255, 255);
}
.home-wrap{
  background: #f2f2f2;
  height: 90rpx;
}

.workbench{
  font-size: 10rpx;
  background: #fff;
  padding-bottom: 0rpx;
  margin-bottom:0rpx;
  color: #333;
}
.workbench .list{
  display: flex;
  flex-direction: row;
  flex:1;
}
.workbench .items{
  width: 120rpx;
  flex:1;
  text-align: center;
}
.workbench .items image{
  width: 80rpx;
  height: 80rpx;
}
.workbench .items image.service-icon{
  width: 120rpx;
  height: 120rpx;
}
.workbench .items image.service-icon1{
  width: 105rpx;
  height: 105rpx;
}
.workbench .items image.service-icon2{
  width: 180rpx;
  height: 100rpx;
}
button[plain]
{ border:0;
  width: 160px;
}
.btn{
  border:0;
  width: 150px;
}
.scroll-box{
  white-space: nowrap;
  height: 100vh;
}
.card-box{
  display: inline-block;
}
 
.card{
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  height: 85vh;
  width: 100vw;
  margin: 0vh 0vw;
  font-size: 40px;
  background: rgb(255, 255, 255);
  border-radius: 0px;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 8%;
  left: 11%;
  opacity: 0.85;
}
 
.modal-dialog {
  overflow: visible;
  position: fixed;
  top: 11%;
  left: 0%;
  margin: 0rpx 5rpx;
  border-radius: 10%;
}
 
.modal-dialog2 {
  overflow: visible;
  position: fixed;
  top: 24%;
  left: 2%;
  margin: 0rpx 5rpx;
  border-radius: 0%;
}
.iconbtn{
  border:0;
  width: 70px;
  margin: 0vh 0vw;
}
.iconbench{
  padding-bottom: 0rpx;
  margin-bottom:0rpx;
  color: #333;
}
.iconbench .list{
  display: flex;
  flex-direction: row;
  flex:1;
}
.iconbench .items{
  width: 100rpx;
  height: 100rpx;
  flex:1;
}
.iconbench .items2{
  width: 210rpx;
  height: 110rpx;
  flex:1;
}
.view-image2{
  width: 90rpx;
  height: 90rpx;
  margin: 0rpx 0rpx;
}

.view-image4{
  width: 180rpx;
  height: 130rpx;
}
.view-image5{
  width: 130rpx;
  height: 70rpx;
}
.view-image6{
  margin:0rpx 0rpx;
}

.view-image{
  width: 290rpx;
  height: 290rpx;
}

